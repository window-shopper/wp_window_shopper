{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/button-has-type": 0,
      "no-console": 0,
      "react/prop-types": 0,
      "react/jsx-props-no-spreading": 0,
      "no-param-reassign": 0
    }
}
